{
  "expo": {
    "name": "campus foodz",
    "slug": "campusfoodz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6995FF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.expo.campusfoodz"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.expo.campusfoodz",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "INTERNET"  
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "097b9af4-48df-453f-8060-444b1c899ed4"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/097b9af4-48df-453f-8060-444b1c899ed4"
    }
  }
}
